{
  "name": "voc-cli",
  "version": "1.5.0",
  "description": "download and play English vocabularies' audio via command line",
  "main": "bin/getAudio.js",
  "preferGlobal": "true",
  "bin": {
    "voc": "index.js"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "babel src --out-dir bin",
    "test": "mocha",
    "lint": "eslint .",
    "cover": "istanbul cover _mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zlargon/voc.git"
  },
  "keywords": [
    "text-to-speech",
    "google-tts",
    "vocabulary",
    "webster",
    "collins",
    "ispeech",
    "voicerss"
  ],
  "author": "zlargon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zlargon/voc/issues"
  },
  "homepage": "https://github.com/zlargon/voc#readme",
  "dependencies": {
    "babel-polyfill": "^6.6.1",
    "cheerio": "^0.19.0",
    "commander": "^2.9.0",
    "node-fetch": "^1.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.6.5",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "coveralls": "^2.11.6",
    "eslint": "^2.1.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.3.4",
    "rewire": "^2.5.1"
  }
}
